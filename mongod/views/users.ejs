<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
  <h1><%= title %></h1>
  <table>
      <tr>
          <th>姓名</th>
          <th>邮箱</th>
      </tr>
      <% for(i=0; i< users.length; i++) {%>
      <tr>
          <td><%= users[i].username %></td>
          <td><a href="mailto:<%= users[i].email %>"><%= users[i].email %></a></td>
      </tr>
      <% } %>
  </table>
  </body>
</html>
